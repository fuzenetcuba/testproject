    {% extends 'BackendBundle::layout.html.twig' %}{% block active_feedback  %}active{% endblock %}{% block page_heading_title %}
    <a class="btn btn-default navbar-btn" href="{{ path('feedback')}}"
       style="margin-bottom: 7px;">
        <span class="glyphicon glyphicon-chevron-up"></span>
    </a>
    Feedback data
    {% endblock %}{% block page_content %}
    <dl class="dl-horizontal">
        <tbody>
            <dt>Id</dt>
                <dd>{{ entity.id }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Satisfied</dt>
                <dd>
                    <div class="onoffswitch-ios switch-xs">
                        {% if (entity.satisfied) %}
                        <label class="onoffswitch-ios-label checked"></label>
                        {% else %}
                        <label class="onoffswitch-ios-label"></label>
                        {% endif %}
                    </div>
                </dd>
            <div class="hr-line-dashed"></div>
            <dt>Recommended</dt>
                <dd>
                    <div class="onoffswitch-ios switch-xs">
                        {% if (entity.recommended) %}
                        <label class="onoffswitch-ios-label checked"></label>
                        {% else %}
                        <label class="onoffswitch-ios-label"></label>
                        {% endif %}
                    </div>
                </dd>
            <div class="hr-line-dashed"></div>
            <dt>Whynotrecommend</dt>
                <dd>{{ entity.whyNotRecommend }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Storessatisfied</dt>
                <dd>
                    <div class="onoffswitch-ios switch-xs">
                        {% if (entity.storesSatisfied) %}
                        <label class="onoffswitch-ios-label checked"></label>
                        {% else %}
                        <label class="onoffswitch-ios-label"></label>
                        {% endif %}
                    </div>
                </dd>
            <div class="hr-line-dashed"></div>
            <dt>Additionalstores</dt>
                <dd>{{ entity.additionalStores }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Foodsatisfied</dt>
                <dd>
                    <div class="onoffswitch-ios switch-xs">
                        {% if (entity.foodSatisfied) %}
                        <label class="onoffswitch-ios-label checked"></label>
                        {% else %}
                        <label class="onoffswitch-ios-label"></label>
                        {% endif %}
                    </div>
                </dd>
            <div class="hr-line-dashed"></div>
            <dt>Additionalfood</dt>
                <dd>{{ entity.additionalFood }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Rate</dt>
                <dd>{{ entity.rate }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Name</dt>
                <dd>{{ entity.name }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Email</dt>
                <dd>{{ entity.email }}</dd>
            <div class="hr-line-dashed"></div>
            <dt>Phone</dt>
                <dd>{{ entity.phone }}</dd>
            <div class="hr-line-dashed"></div>
        </tbody>
    </dl>    <div class="form-group">
    <div class="col-md-2 text-right">
        <a class="btn btn-success" href="{{ path('feedback') }}">
            <span class="glyphicon glyphicon-chevron-left"></span>
            Back to the list
        </a>
    </div>
    <div class="col-md-10">

                
                
                            <a class="btn btn-primary"
               href="{{ path('feedback_edit', { 'id': entity.id }) }}">
                <span class="fa fa-pencil"></span>
                Edit
            </a>
        
                            <a class="btn btn-danger" data-toggle="modal" data-target="#modalDelete">
                <span class="fa fa-remove"></span>
                Delete
            </a>
        
        
            </div>
</div>


    <div class="modal inmodal" id="modalDelete" tabindex="-1" role="dialog" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content animated bounceInRight">
                <div id="modal-title" class="modal-header red-bg">
                    <button type="button" class="close" data-dismiss="modal"><span
                                aria-hidden="true">&times;</span><span class="sr-only">Close</span>
                    </button>
                    <i class="fa fa-exclamation-triangle modal-icon text-white"></i>
                    <h4 class="modal-title">Atention!!!</h4>
                </div>
                <div id="modal-content" class="modal-body">
                    <strong>Are you sure you want to delete this data?</strong>
                </div>
                <div class="modal-footer">
                    <form action="{{ path('feedback_delete', { 'id': entity.id }) }}"
                          name="form-delete" method="post">
                        <button type="button" class="btn btn-white" data-dismiss="modal">Close</button>
                        <button id="modal-btn" type="submit" class="btn btn-danger" name="feedback-id"
                                value="{{ entity.id }}">
                            <span class="glyphicon glyphicon-ok"></span> Save changes
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>


    {% endblock %}